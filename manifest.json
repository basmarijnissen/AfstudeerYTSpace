{
    "name": "obj ext",
    "description": "my ext",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/watch?v=*"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["./background.js"]
    },
    "options-page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.youtube.com/watch?v=*",
        "storage"
    ]
}