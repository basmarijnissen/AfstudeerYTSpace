{
    "name": "SubSpace",
    "description": "my ext",
    "version": "0.2",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/watch?v=*"
            ],
            "js": ["content.bundle.js"],
            "css": ["foreground.css"]
        }
    ],
    "background": {
        "scripts": ["background.bundle.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.youtube.com/watch?v=*",
        "https://www.youtube.com/",
        "storage",
        "activeTab"
    ],
    "web_accessible_resources": ["images/icoon.svg", "icoon.svg", "foreground.css"]
}